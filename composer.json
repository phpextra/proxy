{
    "name": "phpextra/proxy",
    "description": "HTTP Proxy library written in PHP",
    "license": "BSD3-Clause",
    "authors": [
        {
            "name": "Jacek Kobus",
            "email": "kobus.jacek@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "PHPExtra\\Proxy": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "psr/log": "~1.0",
        "doctrine/cache": "~1.3",
        "monolog/monolog": "~1.11",
        "guzzlehttp/guzzle": "~4.0",
        "symfony/http-foundation": "~2.5",
        "symfony/stopwatch": "~2.5",
        "phpextra/event-manager": "~2.0@dev",
        "phpextra/event-manager-silex-provider": "~2.0@dev"
    },
    "require-dev": {
        "silex/silex": "~1.2",
        "phpunit/phpunit": "~4.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "archive": {
        "exclude": ["/build", "/tests", "/docs", "test.bat", "test.php", "phpunit.bat", "fixer.bat", "build.xml"]
    }
}
